{
  "name": "fsd-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:vite": "vite",
    "start:dev": "concurrently \"npm run start:dev:server\" \"npm run start\"",
    "start:dev:vite": "concurrently \"npm run start:dev:server\" \"npm run start:vite\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "cypress open",
    "test:ui": "npx loki test",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ci:tempfix": "concurrently -s first -k \"npm run storybook\" \"npm run test:ui\"",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "prettier": "npx prettier --write **/*.{ts,tsx,json}",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:css": "npx stylelint \"**/*.scss\"",
    "lint:css:fix": "npx stylelint \"**/*.scss\" --fix",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "prepare": "husky install",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "postinstall": "node scripts/clear-cache.js"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": ["prettier --list-different", "eslint"]
  },
  "author": "v.sviridov.it@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
